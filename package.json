{
  "name": "@rodneylab/sveltekit-components",
  "description": "Handy components for building sites in SvelteKit",
  "version": "0.0.4",
  "author": "Rodney Lab <ask@rodneylab.com> (@rodneyLab)",
  "license": "BSD-3-Clause",
  "keywords": [
    "svelte",
    "sveltekit",
    "map",
    "mapbox",
    "openstreetmap"
  ],
  "svelte": "index.js",
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "scripts": {
    "dev": "svelte-kit dev -p 3000",
    "build": "svelte-kit build",
    "preview": "svelte-kit preview -p 3000",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write --plugin-search-dir=. .",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "prepare": "husky install",
    "prettier:check": "prettier --check --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@commitlint/cli": "^13.2.0",
    "@commitlint/config-conventional": "^13.2.0",
    "@sveltejs/kit": "next",
    "@types/leaflet": "^1.7.5",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "husky": "^7.0.0",
    "leaflet": "^1.7.1",
    "prettier": "^2.4.1",
    "prettier-plugin-svelte": "^2.4.0",
    "svelte": "^3.42.6",
    "svelte-check": "^2.2.6",
    "svelte-preprocess": "^4.9.4",
    "svelte2tsx": "^0.4.7",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rodneylab/sveltekit-components"
  },
  "bugs": {
    "url": "https://github.com/rodneylab/sveltekit-components/issues"
  },
  "type": "module"
}
