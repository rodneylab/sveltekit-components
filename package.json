{
  "name": "@rodneylab/sveltekit-components",
  "description": "Handy components for building sites in SvelteKit",
  "version": "0.2.7",
  "author": "Rodney Lab <ask@rodneylab.com> (@rodneyLab)",
  "license": "BSD-3-Clause",
  "keywords": [
    "svelte",
    "sveltekit",
    "map",
    "mapbox",
    "openstreetmap"
  ],
  "svelte": "index.js",
  "scripts": {
    "dev": "svelte-kit dev -p 3030 --https",
    "build": "svelte-kit build",
    "preview": "svelte-kit preview -p 3000",
    "check": "svelte-check --tsconfig ./tsconfig.json --fail-on-hints --ignore package",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write --plugin-search-dir=. .",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "prepare": "husky install",
    "prettier:check": "prettier --check --plugin-search-dir=. .",
    "svelte-check": "svelte-check --fail-on-hints --ignore package"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.0.1",
    "@commitlint/config-conventional": "^16.0.0",
    "@sveltejs/adapter-static": "^1.0.0-next.24",
    "@sveltejs/kit": "next",
    "@types/leaflet": "^1.7.8",
    "@types/node": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "dotenv": "^10.0.0",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.3.0",
    "husky": "^7.0.4",
    "leaflet": "^1.7.1",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.5.1",
    "svelte": "^3.45.0",
    "svelte-check": "^2.2.11",
    "svelte-preprocess": "^4.10.1",
    "svelte2tsx": "^0.4.12",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rodneylab/sveltekit-components"
  },
  "bugs": {
    "url": "https://github.com/rodneylab/sveltekit-components/issues"
  },
  "type": "module"
}
